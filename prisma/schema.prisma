generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String?
  createdAt DateTime @default(now())
}

model Fact {
  id        Int      @id @default(autoincrement())
  claim     String
  answer   String
  createdAt DateTime @default(now())
}
